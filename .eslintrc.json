{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "import/prefer-default-export": "off",
        "react/prefer-stateless-function": "off",
        "linebreak-style": "off",
        "react/prop-types": "off",
        "react/state-in-constructor": "off"
    }
}
